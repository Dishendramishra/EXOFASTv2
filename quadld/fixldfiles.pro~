pro fixldfiles

filters = ['u','b','v','y']


for i=0, n_elements(filters)-1 do begin
   files = file_search('*.' + filter + '.*.sav',nfiles)
   for j=0, nfiles-1 do begin
      split = strsplit(files[i],'.',/extract)
      split[3] = 'Strom' + filters[i]
      newfilename = strjoin(split,'.')
      file_copy, files[i], newfilename
   endfor
endfor

end
